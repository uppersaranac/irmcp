  <xs:element name="Fault">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Code" type="xs:string"/>
        <xs:element name="Message" type="xs:string"/>
        <xs:element name="Details" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="Records">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Record" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="Record">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="RecordType" type="xs:string"/>
        <xs:choice>
          <xs:element name="RecordNumber" type="xs:unsignedInt"/>
          <xs:element name="RecordAccession" type="xs:string"/>
        </xs:choice>
        <xs:element name="RecordTitle" type="xs:string"/>
        <xs:element name="RecordExternalURL" type="xs:string" minOccurs="0"/>
        <xs:element ref="Information" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="Section" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="Reference" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="Section">
    <xs:complexType>
      <xs:sequence>
        <xs:choice>
          <xs:element name="TOCHeading" type="xs:string"/>
          <xs:element name="TOCID" type="xs:int"/>
        </xs:choice>
        <xs:element name="Description" type="xs:string" minOccurs="0"/>
        <xs:element name="URL" type="xs:string" minOccurs="0"/>
        <xs:element ref="DisplayControls" minOccurs="0"/>
        <xs:element ref="Information" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="Section" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="Information">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="ReferenceNumber" type="xs:int"/>
        <xs:element name="Name" type="xs:string" minOccurs="0"/>
        <xs:element name="Description" type="xs:string" minOccurs="0"/>
        <xs:element name="Reference" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="ExtendedReference" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="URL" type="xs:string" minOccurs="0"/>
        <xs:element name="LicenseNote" type="xs:string" minOccurs="0"/>
        <xs:element name="LicenseURL" type="xs:string" minOccurs="0"/>
        <xs:element ref="Value"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <!-- Value and StringWithMarkup and ExtendedReference must be the same as in annotations_3.xsd -->
  <xs:element name="Value">
    <xs:complexType>
      <xs:sequence>
        <xs:choice>
          <xs:element name="Number" type="xs:double" maxOccurs="unbounded"/>
          <xs:element name="DateISO8601" type="xs:string" maxOccurs="unbounded"/>
          <xs:element name="Boolean" type="xs:boolean" maxOccurs="unbounded"/>
          <xs:element ref="StringWithMarkup" maxOccurs="unbounded"/>
          <xs:element name="Binary" type="xs:base64Binary" maxOccurs="unbounded"/>
          <xs:element name="BinaryToStore" type="xs:base64Binary" maxOccurs="unbounded"/>
          <xs:element name="ExternalDataURL" type="xs:string" maxOccurs="unbounded"/>
          <xs:element name="ExternalTableName" type="xs:string"/>
        </xs:choice>
        <xs:element name="Unit" type="xs:string" minOccurs="0"/>
        <xs:element name="MimeType" type="xs:string" minOccurs="0"/>
        <xs:element name="ExternalTableNumRows" type="xs:int" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="StringWithMarkup">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="String" type="xs:string"/>
        <xs:element name="Markup" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Start" type="xs:unsignedLong"/>
              <xs:element name="Length" type="xs:unsignedLong"/>
              <xs:element name="URL" type="xs:string" minOccurs="0"/>
              <xs:element name="Type" type="xs:string" minOccurs="0"/>
              <xs:element name="Extra" type="xs:string" minOccurs="0"/>
              <xs:element name="Size" type="xs:string" minOccurs="0"/>
              <xs:element name="Caption" type="xs:string" minOccurs="0"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="ExtendedReference">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Citation" type="xs:string" minOccurs="0"/>
        <xs:element name="PMID" type="xs:int" minOccurs="0"/>
        <xs:element name="PMCID" type="xs:string" minOccurs="0"/>
        <xs:element name="DOI" type="xs:string" minOccurs="0"/>
        <xs:element name="PublicationTitle" type="xs:string" minOccurs="0"/>
        <xs:element name="PublicationAuthor" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="PublicationName" type="xs:string" minOccurs="0"/>
        <xs:element name="PublicationVolume" type="xs:string" minOccurs="0"/>
        <xs:element name="PublicationIssue" type="xs:string" minOccurs="0"/>
        <xs:element name="PublicationPage" type="xs:string" minOccurs="0"/>
        <xs:element name="PublicationDate" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Year" type="xs:int"/>
              <xs:element name="Month" type="xs:int" minOccurs="0"/>
              <xs:element name="Day" type="xs:int" minOccurs="0"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Matched" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="PCLID" type="xs:int" minOccurs="0"/>
              <xs:element name="Citation" type="xs:string" minOccurs="0"/>
              <xs:element name="PMID" type="xs:int" minOccurs="0"/>
              <xs:element name="PMCID" type="xs:string" minOccurs="0"/>
              <xs:element name="DOI" type="xs:string" minOccurs="0"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <!-- DisplayControls must be the same as in toc_3.xsd -->
  <xs:element name="DisplayControls">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="CreateTable" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="FromInformationIn">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="ThisSection"/>
                    <xs:enumeration value="Subsections"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NumberOfColumns" type="xs:int"/>
              <xs:element name="ColumnHeadings" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
              <xs:choice>
                <xs:element name="ColumnContents" maxOccurs="unbounded">
                  <xs:simpleType>
                    <xs:restriction base="xs:string">
                      <xs:enumeration value="Heading"/>
                      <xs:enumeration value="Name"/>
                      <xs:enumeration value="Value"/>
                      <xs:enumeration value="Reference"/>
                    </xs:restriction>
                  </xs:simpleType>
                </xs:element>
                <xs:element name="ColumnsFromNamedLists">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element name="Name" type="xs:string" maxOccurs="unbounded"/>
                      <xs:element name="UseNamesAsColumnHeadings" type="xs:boolean"/>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:choice>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="ListType" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="Paragraphs"/>
              <xs:enumeration value="CommaSeparated"/>
              <xs:enumeration value="Bulleted"/>
              <xs:enumeration value="Numbered"/>
              <xs:enumeration value="Columns"/>
              <xs:enumeration value="NoBullets"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="HideThisSection" type="xs:boolean" minOccurs="0"/>
        <xs:element name="MoveToTop" type="xs:boolean" minOccurs="0"/>
        <xs:element name="ShowAtMost" type="xs:int" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="Reference">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="ReferenceNumber" type="xs:int"/>
        <xs:element name="SourceName" type="xs:string"/>
        <xs:element name="SourceID" type="xs:string" minOccurs="0"/>
        <xs:element name="Name" type="xs:string" minOccurs="0"/>
        <xs:element name="Description" type="xs:string" minOccurs="0"/>
        <xs:element ref="ExtendedReference" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="URL" type="xs:string" minOccurs="0"/>
        <xs:element name="LicenseNote" type="xs:string" minOccurs="0"/>
        <xs:element name="LicenseURL" type="xs:string" minOccurs="0"/>
        <xs:element name="IsToxnet" type="xs:boolean" minOccurs="0"/>
        <!-- for internal use only -->
        <xs:element name="ANID" type="xs:unsignedInt" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <!-- specialized containers -->

  <xs:element name="Neighbors">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="RecordType" type="xs:string"/>
        <xs:element name="RecordNumber" type="xs:unsignedInt"/>
        <xs:element name="NeighborsOfType" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Type" type="xs:string"/>
              <xs:element name="IDList" type="xs:unsignedInt" maxOccurs="unbounded"/>
              <xs:element name="NameList" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="Structure">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="RecordType" type="xs:string"/>
        <xs:element name="RecordNumber" type="xs:unsignedInt"/>
        <xs:element name="URL" type="xs:string"/>
        <xs:element name="NumberOfStructures" type="xs:int"/>
        <xs:element name="Structures" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="MMDB_ID" type="xs:int"/>
              <xs:element name="PDB_ID" type="xs:string" minOccurs="0"/>
              <xs:element name="URL" type="xs:string"/>
              <xs:element name="ImageURL" type="xs:string"/>
              <xs:element name="Description" type="xs:string" minOccurs="0"/>
              <xs:element name="Taxonomy" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ID" type="xs:int"/>
                    <xs:element name="Name" type="xs:string"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="SourceCategories">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="RecordType" type="xs:string"/>
        <xs:element name="RecordNumber" type="xs:unsignedInt"/>
        <xs:element name="Categories" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Category" type="xs:string"/>
              <xs:element name="URL" type="xs:string"/>
              <xs:element name="Sources" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="SID" type="xs:unsignedInt"/>
                    <xs:element name="SourceName" type="xs:string"/>
                    <xs:element name="SourceURL" type="xs:string" minOccurs="0"/>
                    <xs:element name="SourceDetail" type="xs:string" minOccurs="0"/>
                    <xs:element name="RegistryID" type="xs:string"/>
                    <xs:element name="SourceRecordURL" type="xs:string" minOccurs="0"/>
                    <xs:element name="SourceCategories" type="xs:string" minOccurs="0"
                      maxOccurs="unbounded"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="Literature">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="RecordType" type="xs:string"/>
        <xs:choice>
          <xs:element name="RecordNumber" type="xs:unsignedInt"/>
          <xs:element name="RecordAccession" type="xs:string"/>
        </xs:choice>
        <xs:element name="AllURL" type="xs:string"/>
        <xs:element name="Subheadings" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Subheading" type="xs:string"/>
              <xs:element name="SubheadingURL" type="xs:string"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="Linkout">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="RecordType" type="xs:string"/>
        <xs:element name="RecordNumber" type="xs:unsignedInt"/>
        <xs:element name="ObjUrl" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Url" type="xs:string"/>
              <xs:element name="LinkName" type="xs:string" minOccurs="0"/>
              <xs:element name="SubjectType" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
              <xs:element name="Category" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
              <xs:element name="Attribute" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
              <xs:element name="Provider">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Name" type="xs:string"/>
                    <xs:element name="NameAbbr" type="xs:string"/>
                    <xs:element name="Id" type="xs:string"/>
                    <xs:element name="Url" type="xs:string"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

</xs:schema>
